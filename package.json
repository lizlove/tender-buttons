{
  "name": "tender-buttons",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/lizlove/tender-buttons.git",
  "author": "elizabeth lovero, elizabethlovero.com",
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild-css": "node-sass --include-path scss ./src/sass/main.scss ./src/stylesheets/style.css",
    "min-css": "postcss ./src/stylesheets/style.css --use autoprefixer -b 'last 2 versions' -o ./src/stylesheets/style.css | postcss ./src/stylesheets/style.css --use cssnano -o ./public/stylesheets/style.css",
    "build-css": "yarn run prebuild-css && yarn run min-css",
    "build-app": "webpack -p"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.16",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "body-parser": "^1.17.1",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-include-assets-plugin": "^0.0.5",
    "mailer": "^0.6.7",
    "path": "^0.12.7",
    "postcss-cli": "^3.1.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "request": "^2.81.0",
    "serve-favicon": "^2.4.2",
    "webpack-dev-server": "^2.4.2"
  },
  "description": "Landing page for digital preservation consultants"
}
